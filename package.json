{
  "name": "iniquity-website",
  "version": "1.0.0",
  "description": "The iniquitybbs.org website source code.",
  "scripts": {
    "start": "node node_modules/.bin/concurrently -k --restart-tries 10 --raw \"yarn avian\"",
    "start-prod": "node node_modules/.bin/concurrently -k --restart-tries 25 --raw \"yarn avian\"",
    "avian": "node node_modules/.bin/avian",
    "avian-prod": "node node_modules/.bin/avian --mode production --name iniquity-website --port 80"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iniquitybbs/website.git"
  },
