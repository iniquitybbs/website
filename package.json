{
  "name": "iniquity-website",
  "version": "1.0.0",
  "description": "The iniquitybbs.org website source code.",
  "scripts": {
    "start": "node node_modules/.bin/concurrently -k --restart-tries 10 --raw \"yarn avian\" \"yarn pandoc\"",
    "avian": "node node_modules/.bin/avian",
    "pandoc": "pandoc ./components/index.doc.md -s --toc -c /assets/css/pandoc.css --from markdown --to html5 -o ./assets/html/docs/index.doc.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iniquitybbs/website.git"
  },
  "author": "ispyhumanfly",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iniquitybbs/website/issues"
  },
  "homepage": "https://github.com/iniquitybbs/website#readme",
  "dependencies": {
    "@types/axios": "^0.14.0",
    "@types/vue": "^2.0.0",
    "avian": "https://github.com/flypapertech/avian.git",
    "axios": "^0.18.0",
    "concurrently": "^4.0.1",
    "markdown-it": "^8.4.2",
    "markdown-it-anchor": "^5.0.2",
    "markdown-it-table-of-contents": "^0.4.3",
    "pandoc": "^0.2.0",
    "vue": "^2.5.17",
    "vuetify": "^1.2.3"
  }
}
